{
  "mcpServers": {
    "vercel-domains": {
      "command": "npx",
      "args": [
        "-y",
        "vercel-domains-mcp"
      ],
      "env": {
        "VERCEL_ACCESS_TOKEN": "${VERCEL_ACCESS_TOKEN}"
      }
    },
    "firecrawl": {
      "description": "Firecrawl hosted MCP for authenticated scraping and extraction.",
      "baseUrl": "https://mcp.firecrawl.dev/v2/mcp",
      "headers": {
        "Authorization": "Bearer ${FIRECRAWL_API_KEY}"
      }
    },
    "chrome-devtools": {
      "description": "Attach chrome-devtools-mcp to the live Triangulator browser session.",
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest",
        "--browserUrl",
        "${CHROME_DEVTOOLS_URL}"
      ],
      "lifecycle": "ephemeral"
    },
    "iterm": {
      "command": "/Users/steipete/Projects/iterm-mcp/build/index.js"
    },
    "triangulator": {
      "command": "npx",
      "args": [
        "-y",
        "triangulator",
        "triangulator-mcp"
      ],
      "root": ".."
    },
    "triangulator-local": {
      "description": "Local triangulator-mcp built from dist/bin/triangulator-mcp.js",
      "command": "node",
      "args": [
        "../dist/bin/triangulator-mcp.js"
      ],
      "root": ".."
    }
  },
  "imports": []
}
